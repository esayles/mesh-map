[Unit]
Description=MeshCore Coverage Maintenance

[Service]
Type=oneshot
WorkingDirectory=/home/ubuntu/mesh-map/support/mqtt
ExecStart=/home/ubuntu/venv/bin/python3 -u /home/ubuntu/mesh-map/support/mqtt/wardrive-maint.py
Environment="PYTHONUNBUFFERED=1"
Environment="PATH=/home/ubuntu/venv/bin:/usr/local/bin:/usr/bin"
StandardOutput=journal
StandardError=journal
User=root
Group=root
